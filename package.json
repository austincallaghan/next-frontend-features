{
  "name": "simple-svelte-app",
  "private": true,
  "version": "0.2.0",
  "engines": {
    "node": "20.10.0",
    "yarn": "1.22.x"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "1.22.21"
  },
  "scripts": {
    "dev": "vite dev --open",
    "dev:auth": "vite dev --https --host",
    "build": "vite build",
    "preview": "vite preview --open",
    "dev:validate": "yarn dev & yarn validate --watch",
    "validate": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest --watch",
    "test:ci": "CI=true jest --coverage",
    "coverage:open": "open ./coverage/lcov-report/index.html",
    "coverage:clean": "rm -rf ./coverage",
    "test:e2e": "happo-e2e -- cypress run",
    "cy:open": "yarn cypress open",
    "lint": "eslint . --ext .js,.ts,.svelte --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "formatting": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "formatting:fix": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test:server": "node cypress/server/index",
    "watch:test:server": "nodemon",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@storybook/addon-svelte-csf": "5.0.0-next.14",
    "dotenv": "^16.4.5",
    "jwt-decode": "^4.0.0",
    "postcss": "^8.4.38"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-mdx-gfm": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/svelte": "^8.4.7",
    "@storybook/sveltekit": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.9.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "cypress": "^13.7.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "express": "^4.19.2",
    "happo-cypress": "^4.1.3",
    "happo-e2e": "^2.5.0",
    "happo.io": "^9.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.6",
    "storybook": "^8.4.7",
    "svelte": "^5.10.0",
    "svelte-check": "^4.1.1",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.4"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "type": "module",
  "license": "UNLICENSED"
}
